# CyberAgiesX AI Configuration
# Copy this file to .env and add your actual API keys

# Server Configuration
PORT=3000
WS_PORT=3001
NODE_ENV=development

# OpenAI API (for advanced text analysis)
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Threat Intelligence APIs (optional but recommended for real-time analysis)
# VirusTotal API - Get free API key from https://www.virustotal.com/gui/join-us
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# Google Safe Browsing API - Get from Google Cloud Console
# https://console.cloud.google.com/apis/library/safebrowsing.googleapis.com
GOOGLE_SAFE_BROWSING_API_KEY=your_google_safe_browsing_key_here

# URLScan.io API - Get from https://urlscan.io/user/signup
URLSCAN_API_KEY=your_urlscan_api_key_here

# AbuseIPDB API - Get from https://www.abuseipdb.com/pricing
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here

# Security Configuration
JWT_SECRET=your_jwt_secret_here
RATE_LIMIT_POINTS=100
RATE_LIMIT_DURATION=60

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=cyberagiesx.log

# Database Configuration (PostgreSQL)
DATABASE_URL=postgresql://postgres:password@localhost:5432/cyberagiesx
DB_HOST=localhost
DB_PORT=5432
DB_NAME=cyberagiesx
DB_USER=postgres
DB_PASSWORD=password

# Object Storage (MinIO/S3)
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_BUCKET=cyberagiesx
# Alternative: AWS S3
S3_REGION=us-east-1
S3_BUCKET=cyberagiesx
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
USE_S3=false

# Integration APIs
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WEBHOOK_SECRET=

# Gmail Integration (OAuth2)
GMAIL_CLIENT_ID=
GMAIL_CLIENT_SECRET=
GMAIL_REFRESH_TOKEN=
GMAIL_REDIRECT_URI=http://localhost:3000/api/v1/gmail/oauth/callback
# Gmail Pub/Sub for push notifications (optional)
GMAIL_PUBSUB_TOPIC=

# Email Configuration (for OTP sending)
# Option 1: Custom SMTP Server
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
EMAIL_FROM=noreply@cyberagiesx.com

# Option 2: Gmail App Password (recommended for development)
# Gmail App Password: https://myaccount.google.com/apppasswords
GMAIL_USER=your_email@gmail.com
GMAIL_APP_PASSWORD=your_16_char_app_password

# Option 3: SendGrid API
# SENDGRID_API_KEY=your_sendgrid_api_key

# Feature Flags
DEMO_MODE=false
ENABLE_WEBHOOKS=true

# Gmail Polling (optional)
GMAIL_POLL_INTERVAL=60000
GMAIL_QUERY=is:unread
GMAIL_MAX_RESULTS=10
